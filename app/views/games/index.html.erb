<head>
    <style>
    body {
        background: url(<%= asset_path 'games.jpg' %>) no-repeat center center;
        background-size: cover;
        }
    </style>
</head>


<h1>Games Index</h1>
<ul>
    <% @games.each do |game| %>
        <li>
            <%= link_to game.name, game_path(game) %>
        </li>
    <% end %>
</ul>